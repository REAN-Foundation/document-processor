    "version": "0.2.0",
    "configurations": [
                        
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": ["main:app", "--reload", "--port", "3005"],
            "jinja": false,
            "justMyCode": true
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": true,
            "port": 8000
        }
    ]
}

